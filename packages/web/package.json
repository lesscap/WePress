{
  "name": "@wepress/web",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx watch src/start.ts",
    "build": "prisma generate && tsc",
    "start": "node dist/start.js",
    "postinstall": "prisma generate",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check --write .",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/static": "^8.3.0",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@prisma/client": "^6.19.0",
    "dotenv": "^17.2.1",
    "fastify": "^5.5.0",
    "fastify-plugin": "^5.0.1",
    "fastify-type-provider-zod": "^6.1.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@types/node": "^24.9.2",
    "cross-env": "^10.0.0",
    "prisma": "^6.19.0",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "vitest": "^4.0.6"
  }
}
